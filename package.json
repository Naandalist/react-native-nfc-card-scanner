{
  "name": "react-native-nfc-card-scanner",
  "version": "1.0.0",
  "description": "Scan credit & debit card details (card number, expiry, card type) via NFC contactless reader for React Native (Android & iOS)",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "react-native",
    "nfc",
    "nfc-card-reader",
    "nfc-credit-card",
    "nfc card",
    "card-scanner",
    "card-reader",
    "credit-card",
    "debit-card",
    "payment-card",
    "emv",
    "contactless",
    "tap-to-pay",
    "payment",
    "iso-dep",
    "tlv",
    "android",
    "ios"
  ],
  "author": "Listiananda Apriliawan",
  "username": "Naandalist",
  "licenseUrl": "https://opensource.org/licenses/MIT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Naandalist/react-native-nfc-card-scanner.git"
  },
  "homepage": "https://github.com/Naandalist/react-native-nfc-card-scanner#readme",
  "bugs": {
    "url": "https://github.com/Naandalist/react-native-nfc-card-scanner/issues"
  },
  "peerDependencies": {
    "react-native-nfc-manager": ">=3.14.0"
  },
  "devDependencies": {
    "react-native-nfc-manager": "^3.16.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "yarn@4.12.0+sha512.f45ab632439a67f8bc759bf32ead036a1f413287b9042726b7cc4818b7b49e14e9423ba49b18f9e06ea4941c1ad062385b1d8760a8d5091a1a31e5f6219afca8"
}
